</span><span class='line'><span class="err">　</span> <span class="nx">mothernug</span><span class="p">.</span><span class="o">&lt;</span><span class="nx">em</span><span class="o">&gt;</span><span class="nx">xscale</span> <span class="o">=</span> <span class="mi">150</span><span class="o">;</span>
</span><span class='line'><span class="err">　</span> <span class="nx">mothernug</span><span class="p">.</span><span class="o">&lt;/</span><span class="nx">em</span><span class="o">&gt;</span><span class="nx">yscale</span> <span class="o">=</span> <span class="mi">150</span><span class="o">;</span>
</span><span class='line'><span class="err">　</span> <span class="nx">mothernug</span><span class="p">.</span><span class="nx">_rotation</span> <span class="o">=</span> <span class="nb">Math</span><span class="p">.</span><span class="nx">random</span><span class="p">()</span><span class="o">&lt;/</span><span class="nx">em</span><span class="o">&gt;</span><span class="mi">360</span><span class="o">;</span>
